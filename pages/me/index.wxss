/**index.wxss**/
page{
  font-family: "Microsoft YaHei";
  background-color:#EFEFF4;
}
.section{
  height: 100px;
  background:#28cacc;
  display: flex;
  align-items: center;
  padding: 20rpx 40rpx 20rpx 40rpx;
  box-sizing: border-box;
}
.view-portrait{ 

}
.view-portrait image{
  border: 1px solid #fff;
  width: 60px;
  height: 60px;
  border-radius: 50%;
}

.portrait-content{
  margin-left: 40rpx;
}
.portrait-content div{
  color: #fff;
  font-family: "Microsoft YaHei";
  display: block;
  font-size: 24rpx;
  
}
.portrait-content div:nth-child(1){
  font-size: 36rpx;
  margin-bottom: 16px;
}

.listType{
  margin-top: 20rpx;
}
.classname{
  font-family: "Microsoft YaHei" important;
  background: #fff;  
  font-size: 28rpx;  
  padding: 20rpx;
  border-top: 1px solid #e9e9e9;

}

.Coupon{
  margin-top: 20rpx;
  font-size: 28rpx;
  background: #fff;
  padding: 20rpx;
}

.allOrder:before{
  font-family: 'iconfont';
  content: "\e611";
  margin-right: 20rpx;
  font-size: 32rpx;
}
.PendingPayment:before{
  font-family: 'iconfont';
  content: "\e649";
  margin-right: 20rpx;
  font-size: 32rpx;
}
.received:before{
  font-family: 'iconfont';
  content: "\e609";
  margin-right: 20rpx;
  font-size: 32rpx;
}
.address:before{
  font-family: 'iconfont';
  content: "\e6ab";
  margin-right: 20rpx;
  font-size: 32rpx;
}
.Coupon:before{
  font-family: 'iconfont';
  content: "\e6c7";
  margin-right: 20rpx;
  font-size: 32rpx;
}

.classname i:after{
  font-family: 'iconfont';
  content: "\e612";  
  margin-right: 20rpx;
  font-size: 32rpx;
  float: right;
  font-weight: bold;
  color: #ccc;
}
.Coupon i{
 
  margin-right: 20rpx;
  font-size: 32rpx;
  float: right;
  font-weight: bold;
  color: #ccc;
}
.Coupon i:after{
  font-family: 'iconfont';
  content: "\e612"; 
}